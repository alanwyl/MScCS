<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="a2.cs.hku.hk.msccs">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".StudentActivity" />
        <activity android:name=".ApplicantActivity" />
        <activity android:name=".AlumniActivity" />
        <activity android:name=".OtherActivity" />
        <activity android:name=".StudentCourseActivity" />
        <activity android:name=".StudentHelpActivity" />
        <activity android:name=".StudentLinkActivity"></activity>
    </application>

</manifest>